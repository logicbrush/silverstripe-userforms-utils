{
    "name": "logicbrush/silverstripe-userforms-utils",
    "description": "A spam protector and form field using hCaptcha",
    "homepage": "https://www.logicbrush.com/specialities/silverstripe-development/",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "userforms",
        "utils",
        "utilities",
        "utility"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "John Learn",
            "email": "jflearn@logicbrush.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "silverstripe/framework": "^4.0",
        "silverstripe/userforms": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "extra": {
        "installer-name": "silverstripe-userforms-utils"
    },
    "autoload": {
        "psr-4": {
            "logicbrush\\silverstripe-userforms-utils\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/logicbrush/silverstripe-userforms-utils/issues"
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
